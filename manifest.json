{
  "name": "BITACORA",
  "description": "BITACORA - Control de Acceso",
  "version": "0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "45.0.2439.3",
  "app": {
    "background": {
      "scripts": ["js/background.js"]
    }
  },
  "icons": { "16": "img/access-app-16.png", "128": "img/access-app-128.png" },
  "permissions": [
    "<all_urls>",
    "storage",
	"serial"
  ]
}